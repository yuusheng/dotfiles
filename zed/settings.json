{
  "ui_font_family": "Fira Code",
  "agent": {
    "default_model": {
      "provider": "deepseek",
      "model": "deepseek-chat"
    }
  },
  "theme": "Xcode Classic Dark",
  "vim_mode": true,
  "relative_line_numbers": "enabled",
  "cursor_blink": false,
  "features": {
    "edit_prediction_provider": "zed"
  },
  "autosave": "off",
  "auto_update": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "terminal": {
    "font_size": 14,
    "line_height": {
      "custom": 1.3
    },
    "font_family": "Maple Mono"
  },
  "buffer_font_family": "Maple Mono",
  "inlay_hints": {
    "enabled": true,
    "show_type_hints": true,
    // "show_parameter_hints": true,
    // "show_other_hints": false,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50
  }
}
